<%= debug(params) if Rails.env.development? %>
<% if @tasks_from_date.blank? %>
	<p> No tasks today. </p>
<% else %>
	<p><%= @tasks_from_date.count %> Tasks</p>
	<% @tasks_from_date.each do |task| %>
		<tr>
			<td><%= task.note %></td>
			<td><%= task.completed_at %></td>
		</tr>
	<% end %>
<% end %>

